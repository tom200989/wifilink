<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="WlanSettingTitle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="internet_tv_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dimen_15dip</item>
        <item name="android:textSize">@dimen/dimen_textsize_16sp</item>
    </style>

    <style name="internet_tv_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dimen_15dip</item>
        <item name="android:textSize">@dimen/dimen_textsize_16sp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">@color/gray</item>
    </style>

    <style name="internet_split">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.1dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginLeft">@dimen/dimen_15dip</item>
        <item name="android:layout_marginRight">@dimen/dimen_15dip</item>
        <item name="android:background">@color/gray</item>
    </style>

</resources>
